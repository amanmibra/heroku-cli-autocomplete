{
  "name": "@heroku-cli/plugin-autocomplete",
  "version": "1.0.0",
  "author": "Philipe Navarro @RasPhilCo",
  "bugs": "https://github.com/heroku/heroku-cli-autocomplete/issues",
  "dependencies": {
    "@heroku-cli/command": "^8.1.16",
    "@oclif/command": "1.4.21",
    "@oclif/config": "1.6.18",
    "chalk": "^2.4.1",
    "cli-ux": "^4.2.1",
    "debug": "^3.1.0",
    "fs-extra": "^6.0.1",
    "moment": "^2.22.1",
    "tslib": "1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "1.13.20",
    "@oclif/plugin-help": "1.2.10",
    "@oclif/test": "1",
    "@oclif/tslint": "1.1.1",
    "@types/chai": "4",
    "@types/fs-extra": "^5.0.2",
    "@types/mocha": "5",
    "@types/node": "10.0.8",
    "chai": "4",
    "globby": "8",
    "mocha": "5",
    "nyc": "11.7.3",
    "ts-node": "6.0.3",
    "tslint": "5.10.0",
    "typescript": "2.8"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/.oclif.manifest.json",
    "autocomplete",
    "/lib"
  ],
  "homepage": "https://github.com/heroku/heroku-cli-autocomplete",
  "keywords": [
    "heroku-plugin"
  ],
  "license": "MIT",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "heroku",
    "devPlugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "heroku/heroku-cli-autocomplete",
  "scripts": {
    "postpack": "rm -f .oclif.manifest.json",
    "posttest": "tsc -p test --noEmit && tslint -p test -t stylish",
    "prepack": "rm -rf lib && tsc && oclif-dev manifest && oclif-dev readme",
    "test": "nyc mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  }
}
